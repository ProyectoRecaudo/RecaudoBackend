     *
     * @param float $totaltarifapuesto
     *
     * @return Tmeta
     */
    public function setTotaltarifapuesto($totaltarifapuesto)
    {
        $this->totaltarifapuesto = $totaltarifapuesto;

        return $this;
    }

    /**
     * Get totaltarifapuesto
     *
     * @return float
     */
    public function getTotaltarifapuesto()
    {
        return $this->totaltarifapuesto;
    }

    /**
     * Set totalsaldodeuda
     *
     * @param float $totalsaldodeuda
     *
     * @return Tmeta
     */
    public function setTotalsaldodeuda($totalsaldodeuda)
    {
        $this->totalsaldodeuda = $totalsaldodeuda;

        return $this;
    }

    /**
     * Get totalsaldodeuda
     *
     * @return float
     */
    public function getTotalsaldodeuda()
    {
        return $this->totalsaldodeuda;
    }

    /**
     * Set totalcuotaacuerdo
     *
     * @param float $totalcuotaacuerdo
     *
     * @return Tmeta
     */
    public function setTotalcuotaacuerdo($totalcuotaacuerdo)
    {
        $this->totalcuotaacuerdo = $totalcuotaacuerdo;

        return $this;
    }

    /**
     * Get totalcuotaacuerdo
     *
     * @return float
     */
    public function getTotalcuotaacuerdo()
    {
        return $this->totalcuotaacuerdo;
    }

    /**
     * Set totalsaldodeudaacuerdo
     *
     * @param float $totalsaldodeudaacuerdo
     *
     * @return Tmeta
     */
    public function setTotalsaldodeudaacuerdo($totalsaldodeudaacuerdo)
    {
        $this->totalsaldodeudaacuerdo = $totalsaldodeudaacuerdo;

        return $this;
    }

    /**
     * Get totalsaldodeudaacuerdo
     *
     * @return float
     */
    public function getTotalsaldodeudaacuerdo()
    {
        return $this->totalsaldodeudaacuerdo;
    }

    /**
     * Set totalsaldomultas
     *
     * @param float $totalsaldomultas
     *
     * @return Tmeta
     */
    public function setTotalsaldomultas($totalsaldomultas)
    {
        $this->totalsaldomultas = $totalsaldomultas;

        return $this;
    }

    /**
     * Get totalsaldomultas
     *
     * @return float
     */
    public function getTotalsaldomultas()
    {
        return $this->totalsaldomultas;
    }

    /**
     * Set mesletras
     *
     * @param string $mesletras
     *
     * @return Tmeta
     */
    public function setMesletras($mesletras)
    {
        $this->mesletras = $mesletras;

        return $this;
    }

    /**
     * Get mesletras
     *
     * @return string
     */
    public function getMesletras()
    {
        return $this->mesletras;
    }

    /**
     * Set creacionmeta
     *
     * @param \DateTime $creacionmeta
     *
     * @return Tmeta
     */
    public function setCreacionmeta($creacionmeta)
    {
        $this->creacionmeta = $creacionmeta;

        return $this;
    }

    /**
     * Get creacionmeta
     *
     * @return \DateTime
     */
    public function getCreacionmeta()
    {
        return $this->creacionmeta;
    }

    /**
     * Set modificacionmeta
     *
     * @param \DateTime $modificacionmeta
     *
     * @return Tmeta
     */
    public function setModificacionmeta($modificacionmeta)

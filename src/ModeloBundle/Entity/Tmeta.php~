<?php

namespace ModeloBundle\Entity;

/**
 * Tmeta
 */
class Tmeta
{
    /**
     * @var float
     */
    private $totaltarifapuesto = '0';

    /**
     * @var float
     */
    private $totalsaldodeuda = '0';


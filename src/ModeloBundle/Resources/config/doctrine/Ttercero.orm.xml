<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ModeloBundle\Entity\Ttercero" table="ttercero">
    <id name="pkidtercero" type="integer" column="pkidtercero">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ttercero_pkidtercero_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombretercero" type="string" column="nombretercero" length="225" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identificaciontercero" type="string" column="identificaciontercero" length="225" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefonotercero" type="string" column="telefonotercero" length="225" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creaciontercero" type="datetime" column="creaciontercero" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="modificaciontercero" type="datetime" column="modificaciontercero" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="tipotercero" type="string" column="tipotercero" length="225" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

        $this->totalvalorpuestoeventual = $totalvalorpuestoeventual;

        return $this;
    }

    /**
     * Get totalvalorpuestoeventual
     *
     * @return float
     */
    public function getTotalvalorpuestoeventual()
    {
        return $this->totalvalorpuestoeventual;
    }


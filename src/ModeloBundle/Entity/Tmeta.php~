     * @var float
     */
    private $totalcuotaacuerdo = '0';

    /**
     * @var float
     */
    private $totalsaldodeudaacuerdo = '0';

    /**
     * @var float
     */
    private $totalsaldomultas = '0';

    /**
     * @var string
     */
    private $mesletras;


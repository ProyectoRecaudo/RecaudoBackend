<?php

namespace ModeloBundle\Entity;

/**
 * Tparqueadero
 */
class Tparqueadero
{
    /**
     * @var string
     */
    private $codigoparqueadero;

    /**
     * @var string
     */
    private $numeroparqueadero;


    /**
     * @var boolean
     */
    private $parqueaderoactivo = '1';

    /**
     * @var \DateTime
     */
    private $creacionparqueadero = 'now()';

    /**
     * @var \DateTime
     */
    private $modificacionparqueadero = 'now()';

    /**
     * @var integer
     */
    private $pkidparqueadero;

    /**
     * @var \ModeloBundle\Entity\Tplaza
     */
    private $fkidplaza;

    /**
     * @var \ModeloBundle\Entity\Ttipoparqueadero
     */
    private $fkidtipoparqueadero;


    /**
     * Set codigoparqueadero
     *
     * @param string $codigoparqueadero
     *
     * @return Tparqueadero
     */
    public function setCodigoparqueadero($codigoparqueadero)
    {
        $this->codigoparqueadero = $codigoparqueadero;

        return $this;
    }

    /**
     * Get codigoparqueadero
     *
     * @return string
     */
    public function getCodigoparqueadero()
    {
        return $this->codigoparqueadero;
    }

    /**
     * Set numeroparqueadero
     *
     * @param string $numeroparqueadero
     *
     * @return Tparqueadero
     */
    public function setNumeroparqueadero($numeroparqueadero)
    {
        $this->numeroparqueadero = $numeroparqueadero;

        return $this;
    }

    /**
     * Get numeroparqueadero
     *
     * @return string
     */
    public function getNumeroparqueadero()
    {
        return $this->numeroparqueadero;
    }


    /**
     * Set parqueaderoactivo
     *
     * @param boolean $parqueaderoactivo
     *
     * @return Tparqueadero
     */
    public function setParqueaderoactivo($parqueaderoactivo)
    {
        $this->parqueaderoactivo = $parqueaderoactivo;

        return $this;
    }

    /**
     * Get parqueaderoactivo
     *
     * @return boolean
     */
    public function getParqueaderoactivo()
    {
        return $this->parqueaderoactivo;
    }

    /**
     * Set creacionparqueadero
     *
     * @param \DateTime $creacionparqueadero
     *
     * @return Tparqueadero
     */
    public function setCreacionparqueadero($creacionparqueadero)
    {
        $this->creacionparqueadero = $creacionparqueadero;

        return $this;
    }

    /**
     * Get creacionparqueadero
     *
     * @return \DateTime
     */
    public function getCreacionparqueadero()
    {
        return $this->creacionparqueadero;
    }

    /**
     * Set modificacionparqueadero
     *
     * @param \DateTime $modificacionparqueadero
     *
     * @return Tparqueadero
     */
    public function setModificacionparqueadero($modificacionparqueadero)
    {
        $this->modificacionparqueadero = $modificacionparqueadero;

        return $this;
    }

    /**
     * Get modificacionparqueadero
     *
     * @return \DateTime
     */
    public function getModificacionparqueadero()
    {
        return $this->modificacionparqueadero;
    }

    /**
     * Get pkidparqueadero
     *
     * @return integer
     */
    public function getPkidparqueadero()
    {
        return $this->pkidparqueadero;
    }

    /**
     * Set fkidplaza
     *
     * @param \ModeloBundle\Entity\Tplaza $fkidplaza
     *
     * @return Tparqueadero
     */
    public function setFkidplaza(\ModeloBundle\Entity\Tplaza $fkidplaza = null)
    {
        $this->fkidplaza = $fkidplaza;

        return $this;
    }

    /**
     * Get fkidplaza
     *
     * @return \ModeloBundle\Entity\Tplaza
     */
    public function getFkidplaza()
    {
        return $this->fkidplaza;
    }

    /**
     * Set fkidtipoparqueadero
     *
     * @param \ModeloBundle\Entity\Ttipoparqueadero $fkidtipoparqueadero
     *
     * @return Tparqueadero
     */
    public function setFkidtipoparqueadero(\ModeloBundle\Entity\Ttipoparqueadero $fkidtipoparqueadero = null)
    {
        $this->fkidtipoparqueadero = $fkidtipoparqueadero;

        return $this;
    }

    /**
     * Get fkidtipoparqueadero
     *
     * @return \ModeloBundle\Entity\Ttipoparqueadero
     */
    public function getFkidtipoparqueadero()
    {
        return $this->fkidtipoparqueadero;
    }
}

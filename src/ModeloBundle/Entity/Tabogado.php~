<?php

namespace ModeloBundle\Entity;

/**
 * Tabogado
 */
class Tabogado
{
    /**
     * @var string
     */
    private $codigoabogado;

    /**
     * @var string
     */
    private $nombreabogado;

    /**
     * @var string
     */
    private $direccionabogado;

    /**
     * @var integer
     */
    private $telefonoabogado;

    /**
     * @var boolean
     */
    private $abogadoactivo = '1';

    /**
     * @var \DateTime
     */
    private $creacionabogado = 'now()';

    /**
     * @var \DateTime
     */
    private $modificacionabogado = 'now()';

    /**
     * @var integer
     */
    private $pkidabogado;


    /**
     * Set codigoabogado
     *
     * @param string $codigoabogado
     *
     * @return Tabogado
     */
    public function setCodigoabogado($codigoabogado)
    {
        $this->codigoabogado = $codigoabogado;

        return $this;
    }

    /**
     * Get codigoabogado
     *
     * @return string
     */
    public function getCodigoabogado()
    {
        return $this->codigoabogado;
    }

    /**
     * Set nombreabogado
     *
     * @param string $nombreabogado
     *
     * @return Tabogado
     */
    public function setNombreabogado($nombreabogado)
    {
        $this->nombreabogado = $nombreabogado;

        return $this;
    }

    /**
     * Get nombreabogado
     *
     * @return string
     */
    public function getNombreabogado()
    {
        return $this->nombreabogado;
    }

    /**
     * Set direccionabogado
     *
     * @param string $direccionabogado
     *
     * @return Tabogado
     */
    public function setDireccionabogado($direccionabogado)
    {
        $this->direccionabogado = $direccionabogado;

        return $this;
    }

    /**
     * Get direccionabogado
     *
     * @return string
     */
    public function getDireccionabogado()
    {
        return $this->direccionabogado;
    }

    /**
     * Set telefonoabogado
     *
     * @param integer $telefonoabogado
     *
     * @return Tabogado
     */
    public function setTelefonoabogado($telefonoabogado)
    {
        $this->telefonoabogado = $telefonoabogado;

        return $this;
    }

    /**
     * Get telefonoabogado
     *
     * @return integer
     */
    public function getTelefonoabogado()
    {
        return $this->telefonoabogado;
    }

    /**
     * Set abogadoactivo
     *
     * @param boolean $abogadoactivo
     *
     * @return Tabogado
     */
    public function setAbogadoactivo($abogadoactivo)
    {
        $this->abogadoactivo = $abogadoactivo;

        return $this;
    }

    /**
     * Get abogadoactivo
     *
     * @return boolean
     */
    public function getAbogadoactivo()
    {
        return $this->abogadoactivo;
    }

    /**
     * Set creacionabogado
     *
     * @param \DateTime $creacionabogado
     *
     * @return Tabogado
     */
    public function setCreacionabogado($creacionabogado)
    {
        $this->creacionabogado = $creacionabogado;

        return $this;
    }

    /**
     * Get creacionabogado
     *
     * @return \DateTime
     */
    public function getCreacionabogado()
    {
        return $this->creacionabogado;
    }

    /**
     * Set modificacionabogado
     *
     * @param \DateTime $modificacionabogado
     *
     * @return Tabogado
     */
    public function setModificacionabogado($modificacionabogado)
    {
        $this->modificacionabogado = $modificacionabogado;

        return $this;
    }

    /**
     * Get modificacionabogado
     *
     * @return \DateTime
     */
    public function getModificacionabogado()
    {
        return $this->modificacionabogado;
    }

    /**
     * Get pkidabogado
     *
     * @return integer
     */
    public function getPkidabogado()
    {
        return $this->pkidabogado;
    }
}

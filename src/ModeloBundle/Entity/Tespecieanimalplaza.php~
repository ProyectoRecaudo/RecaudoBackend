<?php

namespace ModeloBundle\Entity;

/**
 * Tespecieanimalplaza
 */
class Tespecieanimalplaza
{
    /**
     * @var \DateTime
     */
    private $creacionespecieanimalplaza = 'now()';

    /**
     * @var integer
     */
    private $pkidespecieanimalplaza;

    /**
     * @var \ModeloBundle\Entity\Tespecieanimal
     */
    private $fkidespecieanimal;

    /**
     * @var \ModeloBundle\Entity\Tplaza
     */
    private $fkidplaza;


    /**
     * Set creacionespecieanimalplaza
     *
     * @param \DateTime $creacionespecieanimalplaza
     *
     * @return Tespecieanimalplaza
     */
    public function setCreacionespecieanimalplaza($creacionespecieanimalplaza)
    {
        $this->creacionespecieanimalplaza = $creacionespecieanimalplaza;

        return $this;
    }

    /**
     * Get creacionespecieanimalplaza
     *
     * @return \DateTime
     */
    public function getCreacionespecieanimalplaza()
    {
        return $this->creacionespecieanimalplaza;
    }

    /**
     * Get pkidespecieanimalplaza
     *
     * @return integer
     */
    public function getPkidespecieanimalplaza()
    {
        return $this->pkidespecieanimalplaza;
    }

    /**
     * Set fkidespecieanimal
     *
     * @param \ModeloBundle\Entity\Tespecieanimal $fkidespecieanimal
     *
     * @return Tespecieanimalplaza
     */
    public function setFkidespecieanimal(\ModeloBundle\Entity\Tespecieanimal $fkidespecieanimal = null)
    {
        $this->fkidespecieanimal = $fkidespecieanimal;

        return $this;
    }

    /**
     * Get fkidespecieanimal
     *
     * @return \ModeloBundle\Entity\Tespecieanimal
     */
    public function getFkidespecieanimal()
    {
        return $this->fkidespecieanimal;
    }

    /**
     * Set fkidplaza
     *
     * @param \ModeloBundle\Entity\Tplaza $fkidplaza
     *
     * @return Tespecieanimalplaza
     */
    public function setFkidplaza(\ModeloBundle\Entity\Tplaza $fkidplaza = null)
    {
        $this->fkidplaza = $fkidplaza;

        return $this;
    }

    /**
     * Get fkidplaza
     *
     * @return \ModeloBundle\Entity\Tplaza
     */
    public function getFkidplaza()
    {
        return $this->fkidplaza;
    }
}

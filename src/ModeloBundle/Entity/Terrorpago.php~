<?php

namespace ModeloBundle\Entity;

/**
 * Terrorpago
 */
class Terrorpago
{
    /**
     * @var float
     */
    private $valorerrorpago;

    /**
     * @var string
     */
    private $resolucionerrorpago;

    /**
     * @var string
     */
    private $documentoerrorpago;

    /**
     * @var boolean
     */
    private $errorpagoactivo = '1';

    /**
     * @var integer
     */
    private $pkiderrorpago;

    /**
     * @var \ModeloBundle\Entity\Tcartera
     */
    private $fkidcartera;


    /**
     * Set valorerrorpago
     *
     * @param float $valorerrorpago
     *
     * @return Terrorpago
     */
    public function setValorerrorpago($valorerrorpago)
    {
        $this->valorerrorpago = $valorerrorpago;

        return $this;
    }

    /**
     * Get valorerrorpago
     *
     * @return float
     */
    public function getValorerrorpago()
    {
        return $this->valorerrorpago;
    }

    /**
     * Set resolucionerrorpago
     *
     * @param string $resolucionerrorpago
     *
     * @return Terrorpago
     */
    public function setResolucionerrorpago($resolucionerrorpago)
    {
        $this->resolucionerrorpago = $resolucionerrorpago;

        return $this;
    }

    /**
     * Get resolucionerrorpago
     *
     * @return string
     */
    public function getResolucionerrorpago()
    {
        return $this->resolucionerrorpago;
    }

    /**
     * Set documentoerrorpago
     *
     * @param string $documentoerrorpago
     *
     * @return Terrorpago
     */
    public function setDocumentoerrorpago($documentoerrorpago)
    {
        $this->documentoerrorpago = $documentoerrorpago;

        return $this;
    }

    /**
     * Get documentoerrorpago
     *
     * @return string
     */
    public function getDocumentoerrorpago()
    {
        return $this->documentoerrorpago;
    }

    /**
     * Set errorpagoactivo
     *
     * @param boolean $errorpagoactivo
     *
     * @return Terrorpago
     */
    public function setErrorpagoactivo($errorpagoactivo)
    {
        $this->errorpagoactivo = $errorpagoactivo;

        return $this;
    }

    /**
     * Get errorpagoactivo
     *
     * @return boolean
     */
    public function getErrorpagoactivo()
    {
        return $this->errorpagoactivo;
    }

    /**
     * Get pkiderrorpago
     *
     * @return integer
     */
    public function getPkiderrorpago()
    {
        return $this->pkiderrorpago;
    }

    /**
     * Set fkidcartera
     *
     * @param \ModeloBundle\Entity\Tcartera $fkidcartera
     *
     * @return Terrorpago
     */
    public function setFkidcartera(\ModeloBundle\Entity\Tcartera $fkidcartera = null)
    {
        $this->fkidcartera = $fkidcartera;

        return $this;
    }

    /**
     * Get fkidcartera
     *
     * @return \ModeloBundle\Entity\Tcartera
     */
    public function getFkidcartera()
    {
        return $this->fkidcartera;
    }
    /**
     * @var \ModeloBundle\Entity\Tasignacionpuesto
     */
    private $fkidasignacionpuesto;


    /**
     * Set fkidasignacionpuesto
     *
     * @param \ModeloBundle\Entity\Tasignacionpuesto $fkidasignacionpuesto
     *
     * @return Terrorpago
     */
    public function setFkidasignacionpuesto(\ModeloBundle\Entity\Tasignacionpuesto $fkidasignacionpuesto = null)
    {
        $this->fkidasignacionpuesto = $fkidasignacionpuesto;

        return $this;
    }

    /**
     * Get fkidasignacionpuesto
     *
     * @return \ModeloBundle\Entity\Tasignacionpuesto
     */
    public function getFkidasignacionpuesto()
    {
        return $this->fkidasignacionpuesto;
    }
}

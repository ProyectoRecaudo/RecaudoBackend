<?php

namespace ModeloBundle\Entity;

/**
 * Ttiposector
 */
class Ttiposector
{
    /**
     * @var integer
     */
    private $pkidtiposector;

    /**
     * @var string
     */
    private $codigotiposector;

    /**
     * @var string
     */
    private $nombretiposector;

    /**
     * @var string
     */
    private $descripciontiposector;

    /**
     * @var boolean
     */
    private $tiposectoractivo;

    /**
     * @var \DateTime
     */
    private $creaciontiposector = 'now()';

    /**
     * @var \DateTime
     */
    private $modificaciontiposector = 'now()';


    /**
     * Get pkidtiposector
     *
     * @return integer
     */
    public function getPkidtiposector()
    {
        return $this->pkidtiposector;
    }

    /**
     * Set codigotiposector
     *
     * @param string $codigotiposector
     *
     * @return Ttiposector
     */
    public function setCodigotiposector($codigotiposector)
    {
        $this->codigotiposector = $codigotiposector;

        return $this;
    }

    /**
     * Get codigotiposector
     *
     * @return string
     */
    public function getCodigotiposector()
    {
        return $this->codigotiposector;
    }

    /**
     * Set nombretiposector
     *
     * @param string $nombretiposector
     *
     * @return Ttiposector
     */
    public function setNombretiposector($nombretiposector)
    {
        $this->nombretiposector = $nombretiposector;

        return $this;
    }

    /**
     * Get nombretiposector
     *
     * @return string
     */
    public function getNombretiposector()
    {
        return $this->nombretiposector;
    }

    /**
     * Set descripciontiposector
     *
     * @param string $descripciontiposector
     *
     * @return Ttiposector
     */
    public function setDescripciontiposector($descripciontiposector)
    {
        $this->descripciontiposector = $descripciontiposector;

        return $this;
    }

    /**
     * Get descripciontiposector
     *
     * @return string
     */
    public function getDescripciontiposector()
    {
        return $this->descripciontiposector;
    }

    /**
     * Set tiposectoractivo
     *
     * @param boolean $tiposectoractivo
     *
     * @return Ttiposector
     */
    public function setTiposectoractivo($tiposectoractivo)
    {
        $this->tiposectoractivo = $tiposectoractivo;

        return $this;
    }

    /**
     * Get tiposectoractivo
     *
     * @return boolean
     */
    public function getTiposectoractivo()
    {
        return $this->tiposectoractivo;
    }

    /**
     * Set creaciontiposector
     *
     * @param \DateTime $creaciontiposector
     *
     * @return Ttiposector
     */
    public function setCreaciontiposector($creaciontiposector)
    {
        $this->creaciontiposector = $creaciontiposector;

        return $this;
    }

    /**
     * Get creaciontiposector
     *
     * @return \DateTime
     */
    public function getCreaciontiposector()
    {
        return $this->creaciontiposector;
    }

    /**
     * Set modificaciontiposector
     *
     * @param \DateTime $modificaciontiposector
     *
     * @return Ttiposector
     */
    public function setModificaciontiposector($modificaciontiposector)
    {
        $this->modificaciontiposector = $modificaciontiposector;

        return $this;
    }

    /**
     * Get modificaciontiposector
     *
     * @return \DateTime
     */
    public function getModificaciontiposector()
    {
        return $this->modificaciontiposector;
    }
}
